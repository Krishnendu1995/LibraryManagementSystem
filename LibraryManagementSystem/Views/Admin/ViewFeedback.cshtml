@model IEnumerable<LibraryManagementSystem.Models.Feedback>
@{
    ViewData["Title"] = "ViewFeedback";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>ViewFeedback</h1>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">SNO</th>
            <th scope="col">FirstName</th>
            <th scope="col">LastName</th>
            <th scope="col">Comments</th>


        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {


        <tr>

            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Comments</td>


        </tr>
        }
    </tbody>
</table>
